# scons build file for cmsg - vxworks
Import('env', 'binDir')

# Files to compile for vxworks
vxfileList = ['cMsgExecutor.c']

# for vxworks all we need is the object
for fileName in vxfileList:
    objectFile = env.Object(env.AppendUnique(CPPPATH = ['.', '#src/regexp', '#src/libsrc', '#src/libsrc++']),
                            source = fileName)
    env.Alias('install', env.Install(target = binDir, source = objectFile))
